@{
    ViewData["Title"] = "Books";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Books</h1>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    View all books
                </div>

                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                            <thead>
                                <tr>
                                    <th style="width:20%">Title</th>
                                    <th style="width:20%">Auther</th>
                                    <th style="width:10%">Genre</th>
                                    <th style="width:10%">ISBN</th>
                                    <th style="width:10%">Copies</th>
                                    <th style="width:10%">Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="odd gradeX">
                                    <td>Trident</td>
                                    <td>Internet Explorer 4.0</td>
                                    <td>Win 95+</td>
                                    <td>4</td>
                                    <td class="text-center">4</td>
                                    <td class="text-center">
                                        <button class="btn btn-primary"><i class="fa fa-edit fa-fw"></i>  </button>
                                        <button class="btn btn-danger"><i class="fa fa-trash fa-fw"></i>  </button>
                                    </td>
                                </tr>
                                <tr class="odd gradeX">
                                    <td>Trident</td>
                                    <td>Internet Explorer 4.0</td>
                                    <td>Win 95+</td>
                                    <td>4</td>
                                    <td class="text-center">4</td>
                                    <td class="text-center">
                                        <button class="btn btn-primary"><i class="fa fa-edit fa-fw"></i>  </button>
                                        <button class="btn btn-danger"><i class="fa fa-trash fa-fw"></i>  </button>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

<script>
    $.ajax({
        type: "GET",
             url: "/BooksModels/Details",
            success: function (data) {
            alert(data);
        },
        error: function (error) {
            alert("error");
        }
    });
</script>