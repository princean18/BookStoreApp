    <script>
        async function dataService(url) {
            try {
                const res = await fetch(url);
                if (!res.ok) {
                    throw new Error('Network response error');
                }
            return res.json();
            } catch (err) {
                console.error('Fetch error:', error);
            }
        }
    </script>

